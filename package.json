{
  "name": "solar-mcp",
  "version": "0.1.4",
  "description": "MCP server for Upstage Solar Pro2 and Solar Mini models",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "solar-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "upstage",
    "solar",
    "llm",
    "ai",
    "model-context-protocol"
  ],
  "author": "serithemage",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serithemage/solar-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/serithemage/solar-mcp/issues"
  },
  "homepage": "https://github.com/serithemage/solar-mcp#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
